{
  "name": "Excel-Generator-App",
  "version": "1.0.0",
  "description": "An Electron (JS) app for Excel Generation.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krishna-kashyap-dev/Excel-Generator-ElectronJs"
  },
  "bugs": {
    "url": "https://github.com/krishna-kashyap-dev/Excel-Generator-ElectronJs/issues"
  },
  "homepage": "https://github.com/krishna-kashyap-dev/Excel-Generator-ElectronJs#readme",
  "author": "Krishna Kashyap",
  "license": "ISC",
  "devDependencies": {
    "electron": "^7.1.2",
    "electron-builder": "^22.11.7"
  },
  "dependencies": {
    "exceljs": "^4.2.1"
  },
  "build": {
    "appId": "excel.generator.electron",
    "productName": "Excel Generator Electron (Js)",
    "win": {
      "target": "nsis",
      "icon": "Icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "artifactName": "Excel-Generator.exe"
    }
  }
}
